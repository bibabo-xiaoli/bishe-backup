.bg-primary {
  background-color: #3F7745;
}
.nav-bar {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 44px 16px 10px;
  background: white;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 100;
  box-sizing: border-box;
  border-bottom: 1px solid #f3f4f6;
}
.icon-nav {
  width: 24px;
  height: 24px;
}
.nav-title {
  font-size: 18px;
  font-weight: bold;
}
